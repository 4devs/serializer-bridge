{
    "name": "fdevs/serializer-bridge",
    "type": "symfony-bridge",
    "description": "FDevs Serializer Bridge",
    "keywords": [
        "serializer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Samusev",
            "email": "andrey_simfi@list.ru",
            "homepage": "http://andrey.4devs.io"
        },
        {
            "name": "4devs Community",
            "homepage": "https://github.com/4devs/serializer-bridge/contributors"
        }
    ],
    "require": {
        "fdevs/serializer": "~1.0",
        "php": ">=5.5.9"
    },
    "require-dev": {
        "symfony/dependency-injection": "^2.8|^3.0|^4.0",
        "symfony/http-kernel": "^2.8|^3.0|^4.0",
        "symfony/framework-bundle": "^3.4|^4.0"
    },
    "suggest": {
        "symfony/framework-bundle": "use with symfony framework",
        "symfony/dependency-injection": "use with dependency injection",
        "symfony/http-kernel": "use with symfony bundle"
    },
    "autoload": {
        "psr-4": {
            "FDevs\\Bridge\\Serializer\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
